{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/echo0039/DV2/main/suicide%20rates%20(gender).csv"},
    "title": "Average Crude Suicide Rates (per 100k population) in Asia (2012 - 2014)",
    "width": "container",
    "height": 320,
    "encoding": {
      "y": {
        "field": "Year",
        "type": "nominal",
        "title": "Year"
      },
      "x": {
        "field": "Average Crude Suicide Rates (per 100k population)",
        "type": "quantitative",
        "axis": {"format": ".2f"},
        "scale": {"domain": [0, 16]}
      }
    },
    "layer": [
      {
        "mark": "bar",
        "encoding": {
          "color": {
            "field": "Gender",
            "type": "nominal",
            "scale": {
              "domain": ["Male", "Female"],
              "range": ["#aec7e8", "#dea5a4"]
            },
            "title": "Gender"
          },
          "tooltip": [
            {"field": "Year", "type": "nominal", "title": "Year"},
            {"field": "Average Crude Suicide Rates (per 100k population)", "type": "quantitative", "format": ".2f"}
          ]
        }
      },
      {
        "encoding": {
          "y": {
            "field": "Year",
            "type": "nominal"
          },
          "x": {
            "field": "Average Crude Suicide Rates (per 100k population)",
            "type": "quantitative"
          },
          "text": {"field": "Average Crude Suicide Rates (per 100k population)", "type": "quantitative", "format": ".2f"}
        },
        "mark": {"type": "text", "align": "right", "dx": -40}
      }
    ]
  }